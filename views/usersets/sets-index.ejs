<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Your Sets</title>
</head>
<body>
    <div id="sets">
        <% sets.forEach((set) => { 
            let terms = set.terms; %>
            <a href="/sets/<%= set._id %> ">Set Name: <%= set.name %></a>
            <p>Description: <%= set.descr %></p>
            <p>Language: <%= set.targetLanguage %></p>
            <% console.log(set); %>
        <% }) %>        
    </div>
</body>
</html>